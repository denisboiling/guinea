<%= form_for @online_request, remote: true do |f| %>
  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :patronymic %>
    <%= f.text_field :patronymic %>
  </div>
  <div class="field">
    <%= f.label :birth_date %>
    <%= f.date_select :birth_date, as: :date_select,
                      start_year: 1900, end_year: Time.now.year %>
  </div>
  <div class="field">
    <%= f.label :registration_address %>
    <%= f.text_field :registration_address %>
  </div>
  <div class="field">
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </div>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <%= f.submit 'Отправить'  %>
<% end %>